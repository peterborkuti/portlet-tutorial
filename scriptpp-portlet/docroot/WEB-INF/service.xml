<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN"
	"http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="hu.borkuti.peter.scriptpp.service">
	<author>Peter Borkuti</author>
	<namespace>scriptpp</namespace>

	<entity name="History" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="historyId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="groupId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="line" type="String" />

		<!-- Order -->

		<order by="desc">
			<order-column name="createDate" />
		</order>

		<!-- Finder methods -->

	</entity>
</service-builder>